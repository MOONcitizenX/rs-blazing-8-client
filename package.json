{
  "name": "rs-clone",
  "version": "1.0.0",
  "description": "Clone of 'Blazing 8s' game built with React",
  "main": "index.js",
  "scripts": {
    "vite": "vite",
    "dev": "run-p styles:watch vite",
    "build": "npm run styles && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "styles": "tcm --outDir types --pattern 'src/**/*.module.css' --camelCase",
    "styles:watch": "npm run styles -- --watch",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkoroleva5/rs-clone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mkoroleva5/rs-clone/issues"
  },
  "dependencies": {
    "@react-spring/types": "^9.6.1",
    "@react-spring/web": "^9.6.1",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^9.0.0",
    "classnames": "^2.3.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.0.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.6.0",
    "sort-by": "^1.2.0",
    "typed-css-modules": "^0.7.2",
    "uuid": "^9.0.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.3",
    "prettier": "2.8.3",
    "typescript": "^4.9.5",
    "vite": "^4.0.0"
  }
}
